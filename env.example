# Cognitive3D Analytics Configuration
# Copy this file to .env and replace the placeholder values with your actual Cognitive3D credentials

# Your Cognitive3D API key from the dashboard
VITE_C3D_API_KEY=your_api_key_here

# The name of your scene in Cognitive3D
VITE_C3D_SCENE_NAME=your_scene_name_here

# The unique ID of your scene (found in Cognitive3D dashboard)
VITE_C3D_SCENE_ID=your_scene_id_here

# Version number for your scene (typically starts at 1)
VITE_C3D_VERSION=1